{
  "name": "weareohmy.com",
  "version": "1.0.0",
  "description": "Front end site for weareohmy.com",
  "main": "index.js",
  "scripts": {
    "build-css": "node-sass assets/sass/ -o dist/css",
    "watch-css": "node-sass -w assets/sass/ -o dist/css",
    "clean": "rm -rf static/css && mkdir -p static/css",
    "build": "npm run clean && npm run build-css",
    "watch": "npm run clean && npm run watch-css & nodemon server -e js,jade",
    "start": "node server"
  },
  "author": "Jack Saunders",
  "license": "ISC",
  "dependencies": {
    "express": "^4.14.0",
    "express-handlebars": "^3.0.0",
    "gulp": "^3.9.1",
    "handlebars": "^4.0.5",
    "morgan": "^1.7.0",
    "node-sass": "^3.8.0"
  },
  "devDependencies": {
    "asset-builder": "^1.1.0",
    "bower": "^1.7.9",
    "browser-sync": "^2.13.0",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-changed": "^1.3.1",
    "gulp-concat": "^2.6.0",
    "gulp-cssnano": "^2.1.2",
    "gulp-if": "^2.0.1",
    "gulp-plumber": "^1.1.0",
    "gulp-rev": "^7.1.0",
    "gulp-sass": "^2.3.2",
    "lazypipe": "^1.0.1",
    "merge-stream": "^1.0.0",
    "minimist": "^1.2.0",
    "nodemon": "^1.9.2",
    "wiredep": "^4.0.0"
  }
}
